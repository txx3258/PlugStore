    <div class="fm">
        <ul class="menu">
            <li><a class="bt3" target="_blank" href="#">&nbsp;&nbsp;&nbsp;发表新讨论</a> </li>
            <li><a class="bt4" href="javascript:void(0);" onclick="GoBottom();">&nbsp;&nbsp;&nbsp;回复此讨论</a> </li>
        </ul>
    </div>

    <div id="comment_container">

    </div>



    </div>
</div>
<%- include _footer %>

<script type="text/javascript">
    (function($){

        var url="/developer/commentList";
        $.my_ajax_html(url,"#comment_container");

        $("#comment_container").delegate(".event a","click",function(){
            //alert('123');

            var replyDom=$(this).parent().next();

            replyDom.show();
            replyDom.html(replyDom.attr("userID"));

            return false;
        });


    })($);



</script>