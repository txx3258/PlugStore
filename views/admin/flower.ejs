<%- include _header %>
<style type="text/css">
    #queryTable{}
    #queryTable tr{height:48px}
    #queryTable tr td{width:150px}
    .wd200{width:200px}
    #row{width: 960px;clear: both;margin: auto}
    .ml_50{margin-left: 50px}
    .ml_10{margin-left: 10px}
    .mt_10{margin-top: 10px}

    #flower_table{margin-left: 10px}
    #flower_table  tr{
        height: 40px;
    }

</style>


<div id="row" class="row">

    <div class="panel panel-default mt_10">
            <div class="panel-heading">
                <h3 class="panel-title"> 查询</h3>
            </div>
            <div class="panel-body">
                <form class="form-signin">
                    <table id="queryTable" class="ml_50">
                        <tr>
                            <td><label>宿主名称：</label></td>
                            <td><select class="form-control wd200">
                                    <option></option>
                                    <option>XP</option>
                                    <option>Windows</option>
                                </select>
                            </td>
                            <td></td>
                            <td><label>插件名称：</label></td>
                            <td><input type="email" id="inputEmail" class="form-control wd200" required autofocus></td>
                        </tr>
                        <tr>
                            <td><label>开发者：</label></td>
                            <td><input type="email" id="inputEmail" class="form-control wd200"  required autofocus></td>
                            <td></td>
                            <td><label>用户名称：</label></td>
                            <td><input type="email" id="inputEmail" class="form-control wd200" required autofocus></td>
                        </tr>

                        <tr>
                            <td><label>所属状态：</label></td>
                            <td>
                                <select class="form-control wd200">
                                    <option></option>
                                    <option>已确认</option>
                                    <option>已下架</option>
                                    <option>已冻结</option>
                                    <option>申请</option>
                                    <option>待测试</option>
                                    <option>通过测试</option>
                                    <option>未测试</option>
                                </select>

                            </td>
                            <td></td>
                            <td><label>插件类别：</label></td>
                            <td><input type="email" id="inputEmail" class="form-control wd200"  required autofocus></td>
                        </tr>

                        <tr>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td>
                                <button class="btn btn-lg btn-primary btn-block" type="button">查询</button>
                            <td>
                        </tr>

                    </table>



                </form>


            </div>
        </div>

    <div class="table-responsive">

        <div id="flower_developer"></div>

        <div id="flower_app"></div>







    </div>

</div>
<div id="bannerBar" class="panel panel-primary" style="width:500px;height:350px;z-index:999999;display: none;">
    <div id="banner" class="panel-heading" style="cursor: move;clear: both;height: 40px; ">
        <h3 class="panel-title" style="float: left">流程审批</h3>
        <span id="queryBoxClose" class="badge" style="float: right;cursor:pointer">&times;</span>
    </div>
    <div class="panel-body">
        <table id="flower_table">
            <tr>
                <td> <input type="radio" name="flower" value="确认" checked="checked"/></td>
                <td><span class="ml_10">确认</span></td>
            </tr>
            <tr>
                <td> <input type="radio" name="flower" value="下架" /></td>
                <td><span class="ml_10">下架</span></td>
            </tr>
            <tr>
                <td> <input type="radio" name="flower" value="下架" /></td>
                <td><span class="ml_10">冻结</span></td>
            </tr>
            <tr>
                <td> <input type="radio" name="flower" value="下架" /></td>
                <td><span class="ml_10">测试中</span></td>
            </tr>
            <tr>
                <td> <input type="radio" name="flower" value="下架" /></td>
                <td><span class="ml_10">通过</span></td>
            </tr>
            <tr>
                <td> <input type="radio" name="flower" value="下架" /></td>
                <td><span class="ml_10">未通过</span></td>
            </tr>


        </table>

        <button style="width: 150px;margin-left: 160px"  class="btn btn-lg btn-primary btn-block" type="button">确定</button>
        <input type="hidden" id="bannerBarholder" value="">
    </div>
</div>


<%- include _footer %>

<script type="text/javascript">

    (function($){
        var url="/admin/flower_developer";
        $.my_ajax_html(url,'#flower_developer');
        $("#flower_developer").delegate("table tr","dblclick",function(){
            $("#bannerBarholder").val($(this).attr("id"));

            $("#bannerBar").show();

            return false;
        });

        var url="/admin/flower_app?type=0&ejsView=flower_app.ejs";
        $.my_ajax_html(url,'#flower_app');
        $("#flower_app").delegate("table tr","dblclick",function(){
            $("#bannerBarholder").val($(this).attr("id"));

            $("#bannerBar").show();

            return false;
        });


    })($);



</script>